{
  "private": true,
  "name": "slick-router",
  "version": "2.5.1",
  "description": "A powerful and flexible client side router",
  "main": "slick-router.js",
  "module": "slick-router.js",
  "types": "types.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "standard | snazzy && node tasks/build.js",
    "lint": "standard",
    "test-coverage": "karma start --browsers ChromeHeadless --single-run",
    "test": "karma start --coverage",
    "test:debug": "karma start --browsers ChromeDebugging --auto-watch=true --single-run=false",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "types": "jsdoc lib/router.js -t node_modules/tsd-jsdoc/dist -r -d build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blikblum/slick-router.git"
  },
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blikblum/slick-router/issues"
  },
  "dependencies": {
    "path-to-regexp": "^5.0.0"
  },
  "keywords": [
    "router",
    "web-components",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-helpers": "^1.4.0",
    "@open-wc/testing-karma": "^3.2.21",
    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "jquery": "^3.4.1",
    "jsdoc": "^3.6.5",
    "karma": "^4.4.1",
    "lit-element": "^2.2.1",
    "mocha": "^6.2.2",
    "rollup": "^1.27.14",
    "rollup-plugin-cpy": "^2.0.1",
    "sinon": "^8.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "tsd-jsdoc": "^2.5.0"
  },
  "standard": {
    "env": {
      "browser": true
    },
    "ignore": [
      "examples/**",
      "docs/**",
      "build/**",
      "lib/locations/location-bar.js",
      "tests/location-bar/**"
    ]
  }
}
