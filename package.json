{
  "private": true,
  "name": "slick-router",
  "version": "2.5.2",
  "description": "A powerful and flexible client side router",
  "main": "slick-router.js",
  "module": "slick-router.js",
  "sideEffects": false,
  "scripts": {
    "build": "standard | snazzy && node tasks/build.js",
    "lint": "standard",
    "start": "web-dev-server --open examples/ --node-resolve",
    "test": "web-test-runner \"tests/**/*Test.js\" --node-resolve",
    "test:coverage": "web-test-runner \"tests/**/*Test.js\" --node-resolve --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blikblum/slick-router.git"
  },
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blikblum/slick-router/issues"
  },
  "dependencies": {
    "path-to-regexp": "^5.0.0"
  },
  "keywords": [
    "router",
    "web-components",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "@open-wc/testing": "^2.5.33",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-import-maps": "^0.0.5",
    "@web/test-runner": "^0.13.13",
    "del": "^6.0.0",
    "jquery": "^3.6.0",
    "lit-element": "^2.5.1",
    "rollup": "^2.52.4",
    "rollup-plugin-cpy": "^2.0.1",
    "sinon": "^11.1.1",
    "snazzy": "^9.0.0",
    "standard": "^16.0.3"
  },
  "standard": {
    "env": {
      "browser": true
    },
    "ignore": [
      "examples/**",
      "docs/**",
      "build/**",
      "lib/locations/location-bar.js",
      "tests/location-bar/**"
    ]
  }
}
